  "private": true,
  "scripts": {
    "build": "VERCEL_FORCE_NO_BUILD_CACHE=1 next build",
    "dev": "next --turbopack",
    "dev:no-cache": "rm -rf .next && NODE_ENV=development next dev --turbopack --no-cache",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "postbuild": "next-sitemap && pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start"
  },
  "dependencies": {
    "@intercom/messenger-js-sdk": "^0.0.15",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@types/three": "^0.180.0",
    "clsx": "^2.1.0",
    "critters": "^0.0.25",
    "framer-motion": "^12.0.0",
    "gray-matter": "^4.0.3",
    "next": "^15.0.2",
    "next-mdx-remote": "^5.0.0",
    "nextra": "workspace:*",
    "nextra-theme-docs": "workspace:*",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "remark-gfm": "^4.0.0",
    "three": "^0.180.0",
    "three-stdlib": "^2.36.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/postcss": "4.1.10",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.8",
    "next-sitemap": "^4.2.3",
    "pagefind": "^1.3.0",
    "tailwindcss": "4.1.10"
  },
